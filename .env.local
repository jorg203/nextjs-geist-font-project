# OpenRouter API Key (obrigatório para funcionalidade completa)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Comet ML API Key (opcional - para tracking de experimentos)
COMET_API_KEY=your_comet_api_key_here

# Perplexity API Key (opcional - para enriquecimento contextual)
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Configurações de otimização
USE_COMET=false
USE_PERPLEXITY=false
USE_CACHE=true

# Site URL para referência
NEXT_PUBLIC_SITE_URL=http://localhost:8000
